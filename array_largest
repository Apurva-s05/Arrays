//Program to find largest value from array.
#include <iostream>
using namespace std;

int main() {
	int n;
	cout<<"Enter number of elements: "<<endl;
	cin>>n;

	int a[n];
	for(int i=0;i<n;i++){
		cin>>a[i];
	}

	int largest=0;
	for(int i=0;i<n;i++)
	if(a[i] > largest){
		largest=a[i];
	}
	cout<<"Largest: "<<largest;


	return 0;
}
