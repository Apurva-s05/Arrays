#include <iostream>
using namespace std;

int main() {
	int a[100][100];
	int m,n;
	cout<<"Enter values of m and n"<<endl;
	cin>>m>>n;

	cout<<"Enter elements in matrix:"<<endl;
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
		cin>>a[i][j];
		}
	}

	//Row-wise
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
		cout<<a[i][j]<<"\t";
		}
		cout<<endl;
	}

	return 0;
}
